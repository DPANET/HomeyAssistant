{
    "name": "Paryes-Timing App",
    "apis": {
        "prayersAPI": {
            "urls": {
                "prayersByCityUrl": {
                    "monthly": "http://api.aladhan.com/v1/calendarByCity",
                    "daily": "http://api.aladhan.com/v1/timingsByCity/:date_or_timestamp"
                },
                "prayersByCoordinatesUrl": {
                    "monthly": "http://api.aladhan.com/v1/calendar",
                    "daily": "http://api.aladhan.com/v1/timings/:date_or_timestamp"
                },
                "prayerMethodsUrl": {
                    "methods": "http://api.aladhan.com/v1/methods"
                }
            },
            "settings": {
                "apiSettings": {
                    "timing": "monthly",
                    "location": "prayersByCoordinatesUrl"
                },
                "adjustments": {
                    "Fajr": 0,
                    "Sunrise": 0,
                    "Dhuhr": 0,
                    "Asr": 0,
                    "Maghrib": 0,
                    "Sunset": 0,
                    "Isha": 0,
                    "Midnight": 0
                },
                "location": {
                    "city": "Abu Dhabi",
                    "countryLong": "United Arab Emirates",
                    "countryCode": "AE"
                },
                "calcuations": {
                    "method": 4,
                    "school": 1,
                    "midnightMode": 0,
                    "latitudeAdjustmentMethod": 3,
                    "hijrAdjustment": 1
                }
            }
        }
     
    },
    "locationProvider": {
        "defaultProvider":"Google"
    }
}